enum Animal: CaseIterable{
//    case cat
//    case dog
    case cat, dog
}

for caseItem in Animal.allCases {
    print(caseItem)
}

print(Animal.allCases.count)

// associated values
enum myAssociated {
    case man(Int, String)
    case woman(Int, Int, Int)
}

var value1 = myAssociated.woman(84, 58, 85)

value1 = myAssociated.man(88, "old man")
value1 = myAssociated.man(99, "old man")
print(value1)

import Foundation

// æšä¸¾ äºº
enum Human {
//    case man
//    case woman
    case man, woman
}

// åŠ¨ç‰©
enum Animal {
//    case cat
//    case dog
    case cat, dog
}

// ç©ºçš„æšä¸¾
enum Nothing {
}

print(type(of: "man and woman and child happy family"))
print(type(of: Human.woman))

// String Human(è¿™æ˜¯ä¸€ç§ç±»å‹)
// enum æ˜¯å¥½ä¸œè¥¿ã€‚ç±»å‹å®‰å…¨ğŸ”ã€‚å½“caseå°‘çš„æ—¶å€™ï¼Œå°½é‡å»ç”¨ã€‚

// Humanå‹çš„å®ä¾‹
var manVar : Human = .man
let womanVar : Human = .woman
manVar = .woman

print(manVar)
print(womanVar)

switch manVar {
case .man:
    print("man")
case .woman:
    break
default:
    print("woman")
}

// æšä¸¾çš„éå† CaseInterable

// associated values
// raw value

enum rawValue: Int {
    case one = 1, two, three
}

print(rawValue.two.rawValue)

enum rawStringValue: String {
    case one, two, three
}

print(rawStringValue.one.rawValue)
