

// swift break in switch
var myswitch = "小芳"
switch myswitch {
case "小明","小宝":
    print("男生")
    fallthrough
case "小芳","小新":
    break
default:
    break
}

// 宏 给编译器看的
if #available(iOS 10, macOS 10.12, *) {
    // Use iOS 10 APIs on iOS, and use macOS 10.12 APIs on macOS
    print("new api")
} else {
    // Fall back to earlier iOS and macOS APIs
    print("old api")
}
